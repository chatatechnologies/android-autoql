<?xml version="1.0" encoding="utf-8"?>
<ScrollView
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_height="match_parent"
	android:layout_width="match_parent"
	android:orientation="vertical">
	<!--
	<chata.can.chata_ai.view.chatDrawer.ChatDrawer
		android:layout_height="match_parent"
		android:layout_width="match_parent"/>-->
	<LinearLayout
		android:gravity="center_horizontal"
		android:layout_height="wrap_content"
		android:layout_width="match_parent"
		android:orientation="vertical">
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/demo_data"/>
		<Switch
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/show_ui_overlay"/>
		<Switch
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/show_drawer_handle"/>
		<Switch
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/show_mask"/>
		<Switch
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/shift_screen_on_open_close"/>
		<Switch
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/theme"/>
		<LinearLayout
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:orientation="horizontal">
			<TextView
				android:background="@color/blue"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="5dp"
				android:text="@string/light"
				android:textColor="@color/white"/>
			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="5dp"
				android:text="@string/dark"/>
		</LinearLayout>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/drawer_placement"/>
		<LinearLayout
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:orientation="horizontal">
			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="5dp"
				android:text="@string/light"/>
			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="5dp"
				android:text="@string/dark"/>
			<TextView
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="5dp"
				android:text="@string/light"/>
			<TextView
				android:background="@color/blue"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="5dp"
				android:text="@string/dark"
				android:textColor="@color/white"/>
		</LinearLayout>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/currency_code"/>
		<EditText
			android:background="@color/white"
			android:gravity="center_horizontal"
			android:layout_height="wrap_content"
			android:layout_width="50dp"
			android:text="USD"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/language_code"/>
		<EditText
			android:background="@color/white"
			android:gravity="center_horizontal"
			android:layout_height="wrap_content"
			android:layout_width="50dp"
			android:text="en-US"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/format_for_month_year"/>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="URL"/>
		<EditText
			android:background="@color/white"
			android:gravity="center_horizontal"
			android:layout_height="wrap_content"
			android:layout_width="200dp"
			android:text="MMM YYYY"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/format_for_day_month_year"/>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="URL"/>
		<EditText
			android:background="@color/white"
			android:gravity="center_horizontal"
			android:layout_height="wrap_content"
			android:layout_width="200dp"
			android:text="MMM DD, YYYY"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/number_of_decimals_for_currency_values"/>
		<EditText
			android:background="@color/white"
			android:gravity="center_horizontal"
			android:layout_height="wrap_content"
			android:layout_width="50dp"
			android:text=""/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/number_of_decimals_for_quantity_values"/>
		<EditText
			android:background="@color/white"
			android:gravity="center_horizontal"
			android:layout_height="wrap_content"
			android:layout_width="50dp"
			android:text=""/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/customer_name"/>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/must_click_reload_drawer_to_apply_this"
			android:textSize="12sp"/>
		<EditText
			android:background="@color/white"
			android:gravity="center_horizontal"
			android:layout_height="wrap_content"
			android:layout_width="200dp"
			android:text="Carlos"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/intro_message"/>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/must_click_reload_drawer_to_apply_this"
			android:textSize="12sp"/>
		<EditText
			android:background="@color/white"
			android:gravity="center_horizontal"
			android:layout_height="wrap_content"
			android:layout_width="200dp"
			android:text=""/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/clear_all_messages_on_close"/>
		<Switch
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/height"/>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/only_for_top_bottom_placement"
			android:textSize="12sp"/>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/must_click_reload_drawer_to_apply_this"
			android:textSize="12sp"/>
		<EditText
			android:background="@color/white"
			android:gravity="center_horizontal"
			android:layout_height="wrap_content"
			android:layout_width="200dp"
			android:text="500"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/width"/>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/only_for_left_right_placement"
			android:textSize="12sp"/>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/must_click_reload_drawer_to_apply_this"
			android:textSize="12sp"/>
		<EditText
			android:background="@color/white"
			android:gravity="center_horizontal"
			android:layout_height="wrap_content"
			android:layout_width="200dp"
			android:text="500"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/title"/>
		<EditText
			android:background="@color/white"
			android:gravity="center_horizontal"
			android:layout_height="wrap_content"
			android:layout_width="200dp"
			android:text="Data Messenger"/>


		<TextView
			android:layout_height="wrap_content"
			android:layout_width="wrap_content"
			android:text="@string/font_family"/>
		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/must_click_reload_drawer_to_apply_this"
			android:textSize="12sp"/>
		<EditText
			android:background="@color/white"
			android:gravity="center_horizontal"
			android:layout_height="wrap_content"
			android:layout_width="200dp"
			android:text="sans-serif"/>

		<TextView
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/chart_colors"/>
		<TextView
			android:gravity="center"
			android:layout_height="wrap_content"
			android:layout_marginEnd="10dp"
			android:layout_marginStart="10dp"
			android:layout_width="wrap_content"
			android:text="@string/msg_array_colors"
			android:textSize="12sp"/>
		<EditText
			android:background="@color/white"
			android:gravity="center_horizontal"
			android:hint="@string/new_color"
			android:layout_height="wrap_content"
			android:layout_width="200dp"
			android:text=""/>

	</LinearLayout>
</ScrollView>