		</activity>

		<activity android:name="chata.can.chata_ai.activity.dataMessenger.DataMessengerActivity">

		</activity>

		<activity android:name=".test.TestActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>

	</application>
</manifest>